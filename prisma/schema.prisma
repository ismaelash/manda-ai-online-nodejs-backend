generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model post {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  title     String
  body      String
  type      String
  date      String
  time      String
  createdAt DateTime @default(now())
}
